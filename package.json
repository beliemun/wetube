{
    "name": "wetube",
    "version": "1.0.0",
    "description": "Cloning Youtube with Valnila and NodeJS",
    "main": "index.js",
    "author": "Brian",
    "dependencies": {
        "@babel/cli": "^7.12.10",
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.11.5",
        "@babel/runtime": "^7.12.5",
        "aws-sdk": "^2.815.0",
        "axios": "^0.21.0",
        "babel-loader": "^8.2.1",
        "body-parser": "^1.19.0",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.5",
        "core-js": "^3.6.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "helmet": "^4.1.1",
        "mongoose": "^5.10.12",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "passport": "^0.4.1",
        "passport-github": "^1.1.0",
        "passport-kakao": "^1.0.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.0.1",
        "pug": "^3.0.0"
    },
    "scripts": {
        "dev:server": "nodemon --exec babel-node src/init.js",
        "dev:assets": "cd src && cross-env WEBPACK_ENV=development && webpack -w",
        "build:assets": "cd src && cross-env WEBPACK_ENV=production && webpack",
        "build:server": "babel src --out-dir build --ignore src/static --ignore src/assets --ignore src/webpack.config.js",
        "copyAll": "shx cp -r src/static build/static && shx cp -r src/views build/views",
        "build": "npm run build:server && npm run build:assets && npm run copyAll",
        "start": "node build/init.js",
        "prestart": "npm run build"
    },
    "devDependencies": {
        "autoprefixer": "^10.0.2",
        "cross-env": "^7.0.3",
        "css-loader": "^5.0.1",
        "eslint": "^7.13.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "mini-css-extract-plugin": "^1.3.0",
        "nodemon": "^2.0.4",
        "postcss": "^8.1.7",
        "postcss-loader": "^4.0.4",
        "prettier": "^2.1.2",
        "sass": "^1.29.0",
        "sass-loader": "^10.0.5",
        "shx": "^0.3.3",
        "webpack": "4.44.2",
        "webpack-cli": "^4.2.0"
    }
}